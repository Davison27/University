package p0;

import java.util.ArrayList;
import java.util.Date;

public class Programacion extends Espectaculo{
	
	ArrayList<Espectaculo> ListaEspectaculos;
	
	
	
	public Programacion() {
		ListaEspectaculos = new ArrayList<Espectaculo>();
	}
	
	public void programarEspectaculo(Espectaculo titulo, Espectaculo categoria, Espectaculo descripcion, Espectaculo fecha, Espectaculo ventas, Espectaculo vendidas) {
		ListaEspectaculos.add(titulo);
		ListaEspectaculos.add(categoria);
		ListaEspectaculos.add(descripcion);
		ListaEspectaculos.add(fecha);
		ListaEspectaculos.add(ventas);
		ListaEspectaculos.add(vendidas);
		
	}
	
	public void verEspectaculos() {
		
	}
	
	public void imprimirEventos() {
		
	}
}
