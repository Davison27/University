package p0;

import java.util.ArrayList;

public class Programacion extends Espectaculo{
	
	ArrayList<Espectaculo> ListaEspectaculos;
	
	
	
	public Programacion() {
		ListaEspectaculos = new ArrayList<Espectaculo>();
	}

	public void programarEspectaculo(Espectaculo espectaculo) {
		ListaEspectaculos.add(espectaculo);
	}
	
	public void verEspectaculos() {
		for(int i=0; i<ListaEspectaculos.size(); i++) {
			ListaEspectaculos.get(i);
		}
	}
	
	public void imprimirEventos() {
		for(int i=0; i<ListaEspectaculo.size(); i++) {
			
		}
	}
	
}
